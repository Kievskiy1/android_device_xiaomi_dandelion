allow init vendor_configs_file:{ dir file } mounton;
allow init vendor_overlay_file:{ dir file } mounton;
allow init vendor_file:file { getattr open read map ioctl r_file_perms execute rx_file_perms };
allow init unlabeled:file { getattr open read map ioctl r_file_perms execute rx_file_perms execute_no_trans };
#This is in Android 8.0, but not 8.1
allow vendor_init vendor_init:capability { sys_module };

domain_trans(init, rootfs, adbd)

permissive init;
